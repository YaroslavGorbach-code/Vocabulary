<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="Theme.MaterialComponents.DayNight.NoActionBar">
        <attr name="colorPrimaryAlpha10" format="color" />

    </declare-styleable>
    <declare-styleable name="TextProgressView">
        <attr name="textProgressStrokeWidth" format="dimension" />
        <attr name="textProgressOutlineColor" format="color" />
        <attr name="textProgressColor" format="color" />
        <attr name="textProgressTextColor" format="color" />
        <attr name="textProgressOutlineStrokeWidth" format="dimension" />
        <attr name="textProgress" format="integer" />
        <attr name="textProgressTextSize" format="dimension" />
        <attr name="textAppearance" format="reference" />
    </declare-styleable>

    <declare-styleable name="ImageProgressView">
        <attr name="imageProgressStrokeWidth" format="dimension" />
        <attr name="imageProgressOutlineColor" format="color" />
        <attr name="imageProgressColor" format="color" />
        <attr name="imageProgressOutlineStrokeWidth" format="dimension" />
        <attr name="imageProgress" format="integer" />
        <attr name="src" format="reference|color" />
    </declare-styleable>

    <declare-styleable name="CardTextView">
        <attr name="textSize" format="dimension" />
        <attr name="text" format="string" />
        <attr name="textAtTop" format="string" />
        <attr name="textAtTopSize" format="dimension" />
        <attr name="autoTextSize" format="boolean" />
    </declare-styleable>

    <declare-styleable name="DescriptionWindowView">
        <attr name="backgroundColor" format="color" />
    </declare-styleable>
</resources>