<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="LetterProgressView">
        <attr name="progressStrokeWidth" format="dimension" />
        <attr name="outlineColor" format="color" />
        <attr name="progressColor" format="color" />
        <attr name="outlineStrokeWidth" format="dimension" />
        <attr name="progress" format="integer" />
        <attr name="textSize" format="dimension" />
    </declare-styleable>

    <declare-styleable name="LetterView">
        <attr name="letterSize" format="dimension" />
        <attr name="letter" format="string" />
        <attr name="description" format="string" />
        <attr name="descriptionSize" format="dimension" />
    </declare-styleable>

    <declare-styleable name="DescriptionWindowView">
        <attr name="backgroundColor" format="color" />
    </declare-styleable>
</resources>